  "license": "ISC",
  "description": "Beenest Platform",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./build/webpack.dev.js",
    "local-dev": "NODE_ENV=local webpack-dev-server --config ./build/webpack.dev.js",
    "build": "NODE_ENV=production webpack -p --config ./build/webpack.prod.js",
    "update": "npm-check -u",
    "test": "jest"
  },
  "dependencies": {
    "@types/googlemaps": "^3.30.8",
    "@types/qs": "^6.5.1",
