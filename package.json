  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.0",
    "autoprefixer": "^8.1.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
