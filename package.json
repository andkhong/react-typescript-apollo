    "@types/zen-observable": "^0.5.3",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link": "^1.2.0",
    "apollo-link-context": "^1.0.6",
    "apollo-link-error": "^1.0.6",
