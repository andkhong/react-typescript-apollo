    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "modulePaths": [
      "/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "cacheDirectory": "<rootDir>/.jestcache/",
    "notify": true,
    "roots": [
      "<rootDir>/test/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src(.*)$": "<rootDir>/src/$1"
    }
  }
